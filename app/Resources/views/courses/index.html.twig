{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
        <div class="row">
            <div class="col">
                <h1>Formation</h1>
                {% for course in results %}
                <p><a href="{{ url('courses.detail' , {'slug' : course.slug} ) }}">  {{ course.formations }}   </a></p>
                {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}